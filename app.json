{
  "expo": {
    "name": "FlashCards questions",
    "description": "My first app with react native",
    "slug": "flashcards",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "org.flashcard.exp"
     },
    "ios": {
      "bundleIdentifier": "org.flashcard.exp",
      "supportsTablet": false
    }
  }
}
